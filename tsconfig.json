{
    "compilerOptions": {
      "baseUrl": ".",
      "paths": { "*": ["types/*"] },
        "allowSyntheticDefaultImports": true,
        "experimentalDecorators": true,
        "jsx": "react",
        "lib": ["dom", "dom.iterable", "es5", "es6", "es2017"],
        "module": "commonjs",
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "outDir": "./dist/",
        "sourceMap": true,
        "strict": true,
        "strictPropertyInitialization": false,
        "target": "es5"
    }
}
